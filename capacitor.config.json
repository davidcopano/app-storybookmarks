{
  "appId": "com.davidcopano.storybookmarks",
  "appName": "Storybookmarks",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "GoogleAuth": {
      "scopes": [
        "profile",
        "email"
      ],
      "serverClientId": "1044159174175-inc7mr1shl8nerb88fk8sq5nr1l65sq3.apps.googleusercontent.com",
      "forceCodeForRefreshToken": true
    }
  },
  "cordova": {
    "preferences": {
      "ScrollEnabled": "false",
      "android-minSdkVersion": "22",
      "android-targetSdkVersion": "30",
      "BackupWebStorage": "none",
      "SplashMaintainAspectRatio": "true",
      "FadeSplashScreenDuration": "300",
      "SplashShowOnlyFirstTime": "false",
      "SplashScreen": "screen",
      "ShowSplashScreenSpinner": "false"
    }
  }
}
